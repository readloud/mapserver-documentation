msgid "`WMS 1.3.0 specification`_"
msgstr ""

#: ../../../en/ogc/wms_server.txt:47
msgid "`WMS Cookbook`_"
msgstr ""

#: ../../../en/ogc/wms_server.txt:48
msgid "`MapServer OGC Web Services Workshop`_ package"
msgstr ""

#: ../../../en/ogc/wms_server.txt:49
msgid ":ref:`MapServer Styled Layer Descriptor (SLD) Howto <sld>`"
msgstr ""

