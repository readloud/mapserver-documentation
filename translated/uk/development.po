#: ../../../en/development/rfc/ms-rfc-111.txt:5
msgid "MS RFC 111: MapCache Support for animated time series"
msgstr ""

#: ../../../en/development/rfc/ms-rfc-111.txt:7
msgid "2014/05"
msgstr ""

#: ../../../en/development/rfc/ms-rfc-111.txt:8
msgid "Jerome Villeneuve-Larouche"
msgstr ""

#: ../../../en/development/rfc/ms-rfc-111.txt:9
msgid "jlarouche@mapgears.com"
msgstr ""

#: ../../../en/development/rfc/ms-rfc-111.txt:11
